<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5.0">
	<name>Phoca - TOPlist Statistics</name>
	<author>Jan Pavelka (www.phoca.cz)</author>
	<creationDate>29/10/2007</creationDate>
	<copyright>Jan Pavelka</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<authorEmail>info@phoca.cz</authorEmail>
	<authorUrl>www.phoca.cz</authorUrl>
	<version>1.0.0 beta</version>
	<description>Phoca - TOPlist Statistics - This module places TOPlist Statistics code into your Joomla! site.</description>
	<files>
		<filename module="mod_phoca_toplist_statistics">mod_phoca_toplist_statistics.php</filename>
		<filename module="mod_phoca_toplist_statistics">index.html</filename>
	</files>
	<languages>
		<language tag="en-GB">en-GB.mod_phoca_toplist_statistics.ini</language>
	</languages>
	<params>
		<param name="toplist_code" type="textarea" rows="15" cols="40" default="" label="TOPlist Code" description="TOPlist Code DESC" />
	</params>
	<params group="advanced">
		<param name="moduleclass_sfx" type="text" default="" label="Module Class Suffix" description="PARAMMODULECLASSSUFFIX" />
		<param name="module_css_style" type="text" default="text-align:center;" label="Module CSS Style" description="Module CSS Style DESC" />
	</params>
</install>
